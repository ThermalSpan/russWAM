%{
    #include "../../src/Loader/rWAMparser.h"
%}

%union {
    int i;
    char* s;
}

%code {
    int yyerror (rWAMparser* parser, const char* s);
    int yylex (YYSTYPE*);

    using namespace std;
}

%defines
%pure-parser
%parse-param { rWAMparser* parser }
%define parse.error verbose
%expect 0



